# Database Configuration
DATABASE_HOST=postgres
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres123
DATABASE_NAME=qdrant_sync

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Qdrant Configuration
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_API_KEY=dev-key-12345

# Gemini AI Configuration (REQUIRED - Get from https://aistudio.google.com/app/apikeys)
# Add your Gemini API key here
GEMINI_API_KEY=your_gemini_api_key_here

# Embedding delay in milliseconds (default: 600ms)
# Reduce to speed up sync, increase if rate limited
GEMINI_EMBEDDING_DELAY_MS=600

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:3011,http://127.0.0.1:3011,http://frontend:3000,http://192.168.18.174:3011,http://192.168.18.97:3011

# Azure AD (Optional - Leave empty if not configured)
AZURE_AD_CLIENT_ID=
AZURE_AD_CLIENT_SECRET=
AZURE_AD_TENANT_ID=
AZURE_AD_REDIRECT_URI=
